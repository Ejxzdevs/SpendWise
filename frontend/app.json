{
  "expo": {
    "name": "Spenwise",
    "slug": "Spenwise",
    "icon": "./assets/images/D.png",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "frontend",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.ejpardillo.spendwise"
    },
    "web": {
      "output": "static"
    },
    "plugins": ["expo-router", "expo-secure-store"],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "59bc5c21-cad1-4ba0-9a81-5535a2394615"
      }
    }
  }
}
